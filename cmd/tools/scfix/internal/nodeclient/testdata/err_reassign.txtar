expected_changes: 1

Test reusing err variable after the Client call.

-- input/test.go --
package test

import (
	"fmt"
	"github.com/smart-core-os/sc-api/go/traits"
	"github.com/smart-core-os/sc-bos/pkg/node"
)

func example(n *node.Node) error {
	var client traits.OnOffApiClient
	err := n.Client(&client)
	if err != nil {
		return err
	}
	err = fmt.Errorf("something else")
	return err
}
-- output/test.go --
